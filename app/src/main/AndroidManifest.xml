<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bjtutravel.bjtutravelagency">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".auth.AuthUiActivity" />
        <activity android:name=".request.create.CreateRequestActivity" />
        <activity android:name=".request.detail.DetailRequestActivity" />
        <activity android:name=".plan.create.CreatePlanActivity" />
        <activity android:name=".plan.detail.DetailPlanActivity" />
        <activity android:name=".web.WebActivity"></activity>
    </application>

</manifest>